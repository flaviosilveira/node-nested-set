{
  "name": "ts-nested-set",
  "version": "1.0.2",
  "description": "Nested Set model implementation for Node.js written in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "del-cli dist",
    "codecov": "codecov -t d9a5a49a-345e-40bf-a22a-7884b47c7924",
    "coverage": "nyc --reporter=html --reporter=text-summary mocha",
    "lint": "tslint -p . -p test",
    "lint:fix": "tslint -p . -p test --fix",
    "prebuild": "npm run clean && npm run lint",
    "precommit": "npm run lint",
    "prepush": "npm run build && npm run coverage",
    "pretest": "npm run build",
    "release": "npm run build && npm publish --access public",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha --exit",
    "test:watch": "mocha --watch-extensions ts --watch"
  },
  "nyc": {
    "per-file": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "lib/nested-set-properties.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "cache": true,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micksatana/node-nested-set.git"
  },
  "keywords": [
    "Nested Set"
  ],
  "author": "Satana Charuwichitratana <micksatana@live.com> (http://www.micksatana.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.7",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.0.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "del-cli": "^1.1.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/node": "^8.0.54"
  }
}
